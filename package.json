{
  "name": "github-preview.nvim",
  "private": true,
  "workspaces": [
    "typescript/*"
  ],
  "scripts": {
    "vi": "nvim",
    "dev:web": "cd typescript/web && bun dev",
    "logs:neovim-bridge": "tail -F -n 0 \"$GP_BRIDGE_LOG_STREAM\"",
    "logs:server": "tail -F -n 0 \"$GP_SERVER_LOG_STREAM\"",
    "clean": "rm -rf node_modules && bun run typescript/recursive.ts clean",
    "format": "bun run typescript/recursive.ts format",
    "lint": "bun run typescript/recursive.ts lint"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "bun-types": "latest",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "gpshared": "*",
    "prettier": "^3.0.2",
    "prettier-plugin-organize-imports": "^3.2.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "bunvim": "^0.0.1"
  }
}
